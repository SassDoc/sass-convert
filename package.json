  ],
  "dependencies": {
    "concat-stream": "^1.4.7",
    "dargs": "^3.0.0",
    "semver": "^4.1.0",
    "semver-regex": "^1.0.0",
    "through2": "^0.6.3",
    "which": "^1.0.5"
  },
  "devDependencies": {
    "eslint": "^0.11.0-alpha.0",
    "faucet": "0.0.1",
    "gulp-rename": "^1.2.0",
    "readdirp": "^1.3.0",
    "rimraf": "^2.2.8",
    "tape": "^3.0.3",
    "traceur": "latest",
    "traceur-runner": "^1.0.2",
    "vinyl-fs": "^0.3.13",
    "vinyl-source-stream": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.1.0"
  },
  "scripts": {
    "rebuild": "rm -rf node_modules && npm install",
    "dist": "rm -rf dist && traceur --modules=commonjs --dir lib dist",
    "runtime": "mkdir -p bin && cp node_modules/traceur/bin/traceur-runtime.js bin/traceur-runtime.js",
    "lint": "eslint lib/*.js",
    "test": "tape test/*.test.js | faucet",
    "pretest": "$npm_package_scripts_prepublish",
    "prepublish": "npm run dist && npm run runtime",
    "inspect": "node test/inspect.js"
  }
}
